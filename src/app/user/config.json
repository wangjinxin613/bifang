{
  "name": "用户管理",
  "id": "user",
  "parentName": "系统",
  "parentId": "system",
  "pages": [
    {
      "id": "list",
      "name": "审批管理列表",
      "type": "list",
      "menu": {
        "show": true,
        "name": "全部"
      },
      "data": {
        "api": "@get('/user/list')",
        "deleteApi": "@get('/activity/delete')",
        "searchForm": [
          {
            "type": "select",
            "selectOptions": "@select('/user/getRoleName','roleName','id')",
            "label": "所属角色",
            "name": "roleName"
          },
          {
            "type": "input",
            "label": "姓名",
            "name": "nickName"
          },
          {
            "type": "dateRange",
            "label": "生日",
            "name": ["birthdayStart", "birthdayEnd"]
          },
          {
            "type": "number",
            "label": "年龄",
            "name": "age"
          }
        ],
        "columns": [
          {
            "title": "用户id",
            "dataIndex": "id"
          },
          {
            "title": "用户名",
            "dataIndex": "username"
          },
          {
            "title": "所属角色",
            "dataIndex": "roleName"
          },
          {
            "title": "姓名",
            "dataIndex": "nickName"
          },
          {
            "title": "性别",
            "dataIndex": "sex"
          },
          {
            "title": "联系方式",
            "dataIndex": "phone"
          },
          {
            "title": "操作",
            "scopedSlots": {
              "customRender": "action"
            },
            "see": true,
            "del": true
          }
        ]
      }
    },
    {
      "id": "form",
      "name": "用户信息",
      "type": "form",
      "data": {
        "createApi": "@post('/activity/deploy')",
        "updateApi": "@post('/activity/deploy')",
        "detailApi": "@get('/activity/selectById')",
        "deleteApi": "@get('/activity/delete')",
        "formOption": [
          {
            "type": "input",
            "label": "用户名",
            "name": "username",
            "required": true
          },
          {
            "type": "select",
            "value": "",
            "label": "复制审核流程",
            "name": "copyFlow",
            "selectOptions": "@select('/activity/getApproveList','activityId','id')"
          },
          {
            "type": "customer",
            "value": "",
            "name": "",
            "label": "审批流设置",
            "width": 8,
            "render": "@customerRender"
          },
          {
            "type": "textarea",
            "value": "",
            "label": "审批说明",
            "name": "describe",
            "width": 18
          }
        ]
      }
    },
    {
      "id": "detail",
      "extend": {
        "id": "form"
      }
    }
  ]
}